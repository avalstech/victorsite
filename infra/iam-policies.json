{
  "OutboxProcessorPolicy": {
    "Version": "2012-10-17",
    "Statement": [
      {
        "Sid": "DynamoReadWriteOutbox",
        "Effect": "Allow",
        "Action": ["dynamodb:Scan", "dynamodb:UpdateItem", "dynamodb:GetItem"],
        "Resource": ["arn:aws:dynamodb:*:*:table/*"]
      },
      {
        "Sid": "SendSES",
        "Effect": "Allow",
        "Action": ["ses:SendEmail", "ses:SendRawEmail", "sesv2:SendEmail"],
        "Resource": "*"
      }
    ]
  }
}
